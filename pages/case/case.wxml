<view class="container">
    <view class="moduleType" wx:for-item="caseItem" wx:for="{{caseList}}">
        <text class="title">{{caseItem.title}}</text>
        <view wx:if="{{caseItem.caseType == 0}}">
            <view class="imageList">
                <image src="{{ig.imageUrl}}" data-designid="{{ig.id}}" data-casetype="{{caseItem.caseType}}" catchtap="toDesignDetail" wx:for-item="ig"
                       wx:for="{{caseItem.designImageList}}" class="imageItem"></image>
            </view>
            <view class="next-page" wx:if="{{caseItem.haveFlag}}">
                <text>更多</text>
                <image src="/static/images/next.png"></image>
            </view>
        </view>
        <view class="imageBig" data-caseitemid="{{caseItem.id}}" data-casetype="{{caseItem.caseType}}" catchtap="toDesignDetail" wx:else>
            <image src="{{caseItem.designImage}}"></image>
        </view>
    </view>
</view>
